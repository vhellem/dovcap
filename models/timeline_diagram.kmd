   <origin-rule min="0" max="1" xlink:role="type" xlink:title="Timeline Event" xlink:href="#oid41" xlink:actuate="user"/>
   <target-rule min="0" max="..." xlink:role="type" xlink:title="Timeline Milestones" xlink:href="#oid31" xlink:actuate="user"/>
  </connect-rule>
  <valueset vset="shared" size="2" xlink:role="type" xlink:title="Event-Milestone" xlink:href="#oid81" xlink:actuate="user">
   <string name="from">compared with</string>
   <string name="to">compared with</string>
  </valueset>
  <valueset vset="default" size="3" xlink:role="type" xlink:title="Event-Milestone" xlink:href="#oid81" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="color" class="volatile"></string>
   <string name="style" class="volatile"></string>
  </valueset>
 </type>

