 <type id="ekaRule" name="ekaRule" title="Rule (EKA)" desc="Represents a rule, an automatic kind of task." usage="object">
  <complex-type-link xlink:role="type" xlink:title="Task (EKA)" xlink:href="eka_task.kmd#ekaTask"/>
  <property name="rulePreCode" xlink:role="text-type" xlink:title="text" xlink:href="metis:std#oid4" label="Input and output parameters" default-visible="top"/>
  <property name="ruleCode" xlink:role="text-type" xlink:title="text" xlink:href="metis:std#oid4" label="Code" default-visible="top"/>
  <property name="ruleInitCode" xlink:role="text-type" xlink:title="text" xlink:href="metis:std#oid4" label="InitCode" default-visible="top"/>
  <property name="rulePostCode" xlink:role="text-type" xlink:title="text" xlink:href="metis:std#oid4" label="PostCode" default-visible="top"/>
