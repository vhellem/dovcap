  <named-method-link name="evaluateRule_" xlink:role="cpp-method" xlink:title="Apply rule" xlink:href="../../rule/operations/rule_methods.kmd#evaluateRule"/>
  <valueset vset="shared" size="5" xlink:role="type" xlink:title="Rule (EKA)" xlink:href="#ekaRule" xlink:actuate="user">
   <string name="troux_shape"></string>
   <string name="troux_color"></string>
   <string name="troux_iconAlias"></string>
   <integer name="troux_terminalForTracing">0</integer>
   <integer name="troux_toplevel">1</integer>
  </valueset>
  <valueset vset="default" size="21" xlink:role="type" xlink:title="Rule (EKA)" xlink:href="#ekaRule" xlink:actuate="user">
