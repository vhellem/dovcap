			set instView        = currentConfig.instView 
        end if
    End Property
    
&#39;-----------------------------------------------------------
    Private Property Get inheritance   &#39;EKA_Inheritance
		if isValid(inh) then  &#39; if internal not valid, then create it ...
			set inheritance = inh
		else
			set currentConfig = new IRTV_Config
