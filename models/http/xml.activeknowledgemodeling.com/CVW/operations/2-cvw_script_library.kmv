    &#39; Types
    Private windowType                   &#39; IMetisType

    &#39; Methods
    Private removeMetamodelMethod        &#39; IMetisMethod
    Private removePartRules              &#39; IMetisMethod

    &#39; Layout strategies
    Private workspaceLayoutStrategy     &#39; IMetisInstance

    &#39; Others
