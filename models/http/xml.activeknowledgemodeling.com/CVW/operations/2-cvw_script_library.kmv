&#39;-----------------------------------------------------------
    Sub addToList(ptr, list, noList)
        dim elem
        dim indx, found

        found = false
        for indx = 1 to noList
            set elem = list(indx)
            if isValid(elem) then
                if ptr.uri = elem.uri then
                    found = true
                    exit for
                end if
            end if
        next
        if not found then
            noList = noList + 1
            ReDim Preserve list(noList)
            set list(noList) = ptr
        end if
    End Sub

&#39;---------------------------------------------------------------------------------------------------
    Function isInList(ptr, list, noList)
        dim i
        for i = 1 to noList
            if list(i).uri = ptr.uri then
                isInList = true
                exit function
            end if
        next
    End Function</string>
