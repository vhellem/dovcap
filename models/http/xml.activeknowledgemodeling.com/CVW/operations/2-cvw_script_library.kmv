   loop
   &#39;titleBarString = mid(titleBarString, 3)
   if titleBarString &#60;&#62; &#34;&#34; then titleBarString = left(titleBarString, len(titleBarString) -2)
   if isEnabled(t) then titleBarString = t.title &#38; &#34; : &#34; &#38; titleBarString
