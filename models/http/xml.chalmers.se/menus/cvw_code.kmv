 <object id="_002asr001bo7fi58314h">
  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">SearchCCinstances</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124442946</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">option explicit

    dim currentModel, currentModelView
    dim currentInstance, currentInstanceView
    dim workarea, workWindow, wObject
    dim actionType
    dim ccType, productType, specificationType
    dim hasInstanceContext2Type, hasContentModelType, anyObjectType
    dim ccObject
    dim compTypeName
    dim ccInstanceType
    dim instances
    dim child, children
    dim cvwSelectDialog, cvwWorkarea


    &#39;Initialization
    set currentModel        = metis.currentModel
    set currentModelView    = currentModel.currentModelView
    set currentInstance     = currentModel.currentInstance
    set currentInstanceView = currentModelView.currentInstanceView

    set ccType              = metis.findType(&#34;http://xml.chalmers.se/class/configurable_component.kmd#configurable_component&#34;)
    set actionType          = metis.findType(&#34;http://xml.activeknowledgemodeling.com/cvw/languages/action_objects.kmd#ObjType_CVW:Button_UUID&#34;)
    set anyObjectType       = metis.findType(&#34;metis:stdtypes#oid1&#34;)
    set productType         = metis.findType(&#34;http://xml.chalmers.se/class/cc_product.kmd#CC_product&#34;)
    set specificationType   = metis.findType(&#34;http://xml.chalmers.se/class/cc_specification.kmd#CC_specification&#34;)
    set hasContentModelType = metis.findType(&#34;http://xml.activeknowledgemodeling.com/cvw/languages/spec_relships.kmd#RelType_CVW:hasModelContext2_UUID&#34;)
    set hasInstanceContext2Type = metis.findType(&#34;http://xml.activeknowledgemodeling.com/cvw/languages/spec_relships.kmd#RelType_CVW:hasInstanceContext2_UUID&#34;)

&#39;stop

    set ccObject = getCcObject(currentInstance, currentInstanceView)
    if isEnabled(ccObject) then
        set workWindow = getWorkWindowView(currentInstance, currentInstanceView)
        set wObject = workWindow.instance
        if isEnabled(wObject) then
            &#39; Remove content
            set children = workWindow.children
            for each child in children
                call currentModelView.deleteObjectView(child)
            next
            &#39; Find new content
            compTypeName = getCcParameterValue(ccObject, &#34;Wire type&#34;)
            set ccInstanceType = new CC_InstanceType
            set ccInstanceType.productType = productType
            set ccInstanceType.productInstType = specificationType
            set instances = ccInstanceType.findInstances(ccObject, compTypeName)
            if isValid(instances) then
                if instances.count &#62; 1 then
                    set cvwSelectDialog = new CVW_SelectDialog
                    cvwSelectDialog.singleSelect = true
                    cvwSelectDialog.title = &#34;Select instances&#34;
                    cvwSelectDialog.heading = &#34;Select instances&#34;
                    set instances = cvwSelectDialog.show(instances)
                end if
                &#39; Populate with new content
                set cvwWorkarea = new CVW_Workarea
                set cvwWorkarea.WorkWindow = workWindow
                call cvwWorkarea.populate(instances, -1)
                set children = workWindow.children
                for each child in children
                    child.open
                next
            end if
        end if
    end if
    
&#39; End

</string>
  </valueset>
 </object>

 <object id="_002asr001bo7k9rs5i05">
  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">ConfigureVariant</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124443087</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">option explicit

dim ccGlobals, ccProject, ccConfig
dim ccObject
dim projectObject
dim currentModel, currentModelView
dim currentInstance, currentInstanceView

&#39;stop

set ccGlobals = new CC_Globals
set ccGlobals = Nothing

set currentModel = metis.currentModel
set currentModelView = currentModel.currentModelView
set currentInstance = currentModel.currentInstance
set currentInstanceView = currentModelView.currentInstanceView

if isValid(currentInstanceView) then
    set ccObject = getCcObject(currentInstance, currentInstanceView)
    if isEnabled(ccObject) then
        set ccConfig = new CC_Configure
        call ccConfig.configureVariant(ccObject)
        set ccConfig = Nothing
        MsgBox &#34;Done!&#34;
    end if
end if
</string>
  </valueset>
 </object>

