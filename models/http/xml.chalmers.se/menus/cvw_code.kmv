   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124440874</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">&#39;option explicit
&#39;[0] ----------------------------------------------------------
dim model, modelView, instView
dim  instParentView

&#39;[1] ----------------------------------------------------------
set model = metis.currentModel
set modelView = model.currentModelView
set instView = modelView.currentInstanceView

&#39;[4] --------- MAIN -------------------------------------------------------------------
  set instParentView = instView.parent.parent
   call removeWinList(instParentView)

&#39;----------------------------------------------------------------------------------------------------------------------
     Sub removeWinList(instWinView) 
        &#39;[a]-------------------------------
        Dim  winView
        &#39;[b]-------------------------------  
         For each winView in instWinView.parent.children         
                   if winView.uri &#60;&#62; instWinView.uri THEN
                      &#39;call modelView.deleteObjectView(winView) 
                      call model.deleteObject(winView.instance) 
                   end if
            next
    
      End Sub



</string>
  </valueset>
 </object>

 <object id="_002asr001bo5i8psajst">
  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">DoLoadWindow</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124440964</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">&#39;option explicit
&#39;[0] ----------------------------------------------------------
dim model, modelView, inst, instView
dim cvwWin, workarea
dim winType, buttonType 

dim cvwStatusBar
dim  instParentView, winName
dim argObj, argVal, InputContainerType, InputContainerName 

dim workSpaceLayout, winLayout, winSymbol, titleBarLayout,titleBarSymbol, workAreaLayout, workAreaSymbol    
dim workSpaceLayoutStrategyType, winLayoutStrategyType, titleBarLayoutStrategyType, workAreaLayoutStrategyType
&#39;[1] ----------------------------------------------------------
set model = metis.currentModel
set inst = model.currentInstance
set modelView = model.currentModelView
set instView = modelView.currentInstanceView

&#39;[2] ------INPUT ARGUMENTS----------------------------------------------------

set argObj          = new CVW_ArgumentValue

&#39; - generic CVWWin  input settings --------------------------------

set argVal       = argObj.findArgument(inst, &#34;WorkspaceLayout&#34;)
workSpaceLayout      = argVal.getNamedStringValue(&#34;value&#34;)

set argVal       = argObj.findArgument(inst, &#34;WindowLayout&#34;)
winLayout      = argVal.getNamedStringValue(&#34;value&#34;)

set argVal       = argObj.findArgument(inst, &#34;WindowSymbol&#34;)
winSymbol      = argVal.getNamedStringValue(&#34;value&#34;)

set argVal            = argObj.findArgument(inst, &#34;TitleBarLayout&#34;)
titleBarLayout      = argVal.getNamedStringValue(&#34;value&#34;)

set argVal            = argObj.findArgument(inst, &#34;TitleBarSymbol&#34;)
titleBarSymbol      = argVal.getNamedStringValue(&#34;value&#34;)

set argVal            = argObj.findArgument(inst, &#34;WorkareaLayout&#34;)
workAreaLayout      = argVal.getNamedStringValue(&#34;value&#34;)

set argVal            = argObj.findArgument(inst, &#34;WorkareaSymbol&#34;)
workAreaSymbol      = argVal.getNamedStringValue(&#34;value&#34;)

&#39;[3] ----------------------------------------------------------
set winType      = metis.findType(&#34;http://xml.activeknowledgemodeling.com/cvw/languages/view_objects.kmd#UiType_CVW:Workarea_UUID&#34;)
set buttonType = metis.findType(&#34;http://xml.activeknowledgemodeling.com/cvw/languages/action_objects.kmd#ObjType_CVW:Button_UUID&#34;)
set cvwWin      = new CVW_Window

&#39;[4] --------- MAIN -------------------------------------------------------------------
&#39;[4a] ---------  Get Menu View  ------------------------------------------------
 set instParentView = cvwWin.getInstView(winType ,&#34;name&#34;,&#34;CVW_Workspace&#34;)
 winName =  inst.title

&#39;[4b] ---------  Remove existing Menu ------------------------------------------------
  &#39;if cvwWin.find(winName, winType, instParentView ) then
&#39;stop 
   &#39;cvwWin.clean
    &#39;cvwWin.remove
  &#39;end if

&#39;[4c] ---------  Create and Populate Menu ------------------------------------------------
  if cvwWin.create(winName, winType, instParentView ) then
        call cvwWin.addSubWindow(&#34;WinTitleBar&#34;, inst.title, winType)
        call populateToolbar(cvwWin.objectView.children(1), &#34;CVW_WinToolbar&#34; ,buttonType) 
        
       call cvwWin.addSubWindow(&#34;WorkArea&#34;, &#34;WorkArea_[&#34;&#38; inst.title &#38;&#34;]&#34;, winType)
 
       &#39;[4c] --------- Layout ------------------------------------------------
       set  workSpaceLayoutStrategyType          = metis.findLayoutStrategy(workSpaceLayout)        
       with  instParentView
                            .layoutStrategy  =  workSpaceLayoutStrategyType                           
        end with

        set  winLayoutStrategyType          = metis.findLayoutStrategy(winLayout)
        with cvwWin.objectView
                            .layoutStrategy  =  winLayoutStrategyType                            
                            .openSymbol    = winSymbol 
                            .closedSymbol  = winSymbol
                            .absTextScale = 1
        end with

        set titleBarLayoutStrategyType      = metis.findLayoutStrategy(titleBarLayout)  
        with cvwWin.objectView.children(1)
                            .layoutStrategy  =  titleBarLayoutStrategyType                            
                            .openSymbol    = titleBarSymbol 
                            .closedSymbol  = titleBarSymbol
                            .absTextScale = 30
                            .geometry.height = 100 
        end with
         set workAreaLayoutStrategyType  = metis.findLayoutStrategy(workAreaLayout)
         with cvwWin.objectView.children(2)
                            .layoutStrategy  =  workAreaLayoutStrategyType                           
                            .openSymbol    = workAreaSymbol 
                            .closedSymbol  = workAreaSymbol
                            .absTextScale = 200
        end with



      call cvwWin.doParentLayout 
  end if

&#39;[4d] ---------  Populate Status Bar------------------------------------------------
 &#39; set cvwStatusBar = new CVW_StatusBar
 &#39; cvwStatusBar.topMenuStatus(instView)
 &#39; cvwStatusBar.populateStatusBars(instView)

&#39;-----------------------------------------------------------------------------------------------------



&#39;----------------------------------------------------------------------------------------------------------------------
     Sub populateToolbar(menuNodeView, menuName, menuNodeTargetType)
    &#39;[a]-------------------------------
        Dim objectMenu
        Dim  menuItemView, newMenuItem, newMenuItemView 
        &#39;[b]-------------------------------  
        Set objectMenu   = model.findInstances(menuNodeTargetType, &#34;name&#34; ,menuName)    
        &#39;[c]-------------------------------  
         For each menuItemView in objectMenu.item(1).neighbourRelationships         
                      if menuItemView.target.title &#60;&#62; menuNodeView.title THEN
                       Set newMenuItemView                         = menuNodeView.newObjectView(menuItemView.target)
                       newMenuItemView.openSymbol    = menuItemView.target.Views(1).openSymbol
                       newMenuItemView.closedSymbol  = menuItemView.target.Views(1).closedSymbol
                       newMenuItemView.textScale         = 0.05
                       newMenuItemView.geometry.width = newMenuItemView.parent.geometry.width * 1.3
                       newMenuItemView.geometry.height = newMenuItemView.parent.geometry.height * 1.3
                       newMenuItemView.close
                       newMenuItemView.parent.open
                    end if
            next
    End Sub



</string>
  </valueset>
 </object>

