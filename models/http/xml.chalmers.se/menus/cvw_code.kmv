 <object id="_002asr001bo51v7oumgo">
  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">DoMenuAction</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124440483</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">option explicit

dim cvwAction, cvwWorkspace
dim ekaInst
dim workspaceWindow
dim layoutStrategy
dim inst, value
dim model
dim applyFilter

&#39;stop

set model = metis.currentModel
set ekaInst = new EKA_Instance
set inst = ekaInst.findObject(model, &#34;WorkspaceContext&#34;)

applyFilter = false
if isEnabled(inst) then
    value = ekaInst.getPropertyValue(inst, &#34;ApplyConfiguration&#34;)
    if value = &#34;1&#34; then
        applyFilter = true
    end if
end if
&#39;stop
set cvwAction = new CVW_MenuAction
call cvwAction.build
cvwAction.applyFilter = applyFilter
set cvwWorkspace = cvwAction.execute

set ekaInst = Nothing
set cvwAction = Nothing</string>
  </valueset>
 </object>

