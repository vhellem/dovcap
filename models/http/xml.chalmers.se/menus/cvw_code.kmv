  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">DoShowRelationshipView</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124440583</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">set model = metis.currentModel
set modelView = model.currentModelView
set inst = model.currentInstance
set instView = modelView.currentInstanceView

if inst.name = &#34;Show Relationships&#34; then
    set cvwWorkspace = new CVW_Workspace
    mode = &#34;ShowInter&#34;
    call cvwWorkspace.showRelationships(mode)
    inst.name = &#34;Hide Relationships&#34;
    instView.close
else
    set cvwWorkspace = new CVW_Workspace
    mode = &#34;HideInter&#34;
    call cvwWorkspace.showRelationships(mode)
    inst.name = &#34;Show Relationships&#34;
    instView.open
end if
set cvwWorkspace  = Nothing
</string>
  </valueset>
 </object>

