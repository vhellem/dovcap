 <object id="_002asr001bo6nfs4olbr">
  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">Toggle Configuration flag</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124442185</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">set currentModel = metis.currentModel
set currentInstance = currentModel.currentInstance

set hasSpecificationType = metis.findType(&#34;http://xml.activeknowledgemodeling.com/cvw/languages/spec_relships.kmd#RelType_CVW:hasSpecification1_UUID&#34;)
set ekaPropertyType = metis.findType(&#34;http://xml.activeknowledgemodeling.com/eka/languages/eka_property.kmd#ObjType_EKA:Property_UUID&#34;)

set relships = currentInstance.getNeighbourRelationships(0, hasSpecificationType)
if relships.count &#62; 0 then
    set specObject = relships(1).target

    set parts = specObject.parts
    for each part in parts
        if part.type.inherits(ekaPropertyType) then
            if part.title = &#34;ApplyConfiguration&#34; then
                applyFilter = part.getNamedStringValue(&#34;value&#34;)
                applyFilter = InputBox(&#34;Apply configuration (1 = yes / 0 = no&#34;,, applyFilter)
                &#39;if applyFilter = 0 then
                &#39;    applyFilter = 1
                &#39;else
                &#39;    applyFilter = 0
                &#39;end if
                set val = metis.newValue
                call val.setInteger(applyFilter)
                call part.setNamedValue(&#34;value&#34;, val)
            end if
        end if
    next
end if</string>
  </valueset>
 </object>

 <object id="_002asr001bo6s5ilaoi3">
  <valueset size="10" xlink:role="type" xlink:title="VB Script (CVW)" xlink:href="http://xml.activeknowledgemodeling.com/cvw/languages/script_types.kmd#ObjType_AKM:VBScript_UUID" xlink:actuate="user">
   <string name="externalID"></string>
   <string name="name">ConfigureWorkspaceContent</string>
   <string name="description"></string>
   <integer name="dbms-admin.commitFlag">1</integer>
   <date-time name="dbms-admin.commitTime">00000000 000000000</date-time>
   <date-time name="dbms-admin.system-modifiedTime">20070924 124442336</date-time>
   <integer name="dbms-admin.system-uploaded">0</integer>
   <string name="parentId"></string>
   <string name="comments"></string>
   <string name="script">option explicit

dim parentView, workWindow
dim cvwWorkspace, ccRule
dim workarea, workareas
dim obj, indx
dim objectView, objectViews
dim didSomething
dim ccType
dim i

set ccType = metis.findType(&#34;http://xml.chalmers.se/class/configurable_component.kmd#configurable_component&#34;)
set cvwWorkspace = new CVW_Workspace
call cvwWorkspace.build
set ccRule = new CC_Rule
for i = 1 to 2
    didSomething = false
    set parentView = cvwWorkspace.WorkspaceWindow
    if isValid(parentView) then
        set workareas = parentView.children
        for each workarea in workareas
            indx = workarea.children.count
            if indx &#62; 0 then
                set workWindow = workarea.children(indx)
                if isValid(workWindow) then
                    set objectViews = workWindow.children
                    if isValid(objectViews) then
                        for each objectView in objectViews
                            if hasInstance(objectView) then
                                set obj = objectView.instance
                                if obj.type.uri = ccType.uri then
                                    call ccRule.configureTopCC(obj)
                                    didSomething = true
                                else
                                    call ccRule.transformRulesToScripts(obj)
                                    call ccRule.ruleEngine.executeRules(obj)
                                    didSomething = true
                                end if
                            end if
                        next
                    end if
                end if
            end if
        next
    end if
next
set ccRule = Nothing
set cvwWorkspace = Nothing
</string>
  </valueset>
 </object>

