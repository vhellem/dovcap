  <property name="description" xlink:role="text-type" xlink:title="Description with Rich Text" xlink:href="metis:std#oid43" label="Description" desc="Enter a description of the object" description-flag="true"/>
  <property name="phoneNr" xlink:role="string-type" xlink:title="PhoneNr" xlink:href="metis:stdprimtypes#oid5" label="Phone no." desc="Telephone number used to contact the organization"/>
  <property name="faxNr" xlink:role="string-type" xlink:title="PhoneNr" xlink:href="metis:stdprimtypes#oid5" label="Fax no." desc="Fax number"/>
  <property name="country" xlink:role="string-type" xlink:title="Name with Rich Text" xlink:href="metis:std#oid42" label="Country"/>
  <property name="x400" xlink:role="string-type" xlink:title="Name with Rich Text" xlink:href="metis:std#oid42" label="X-400"/>
  <property name="email" xlink:role="string-type" xlink:title="Name with Rich Text" xlink:href="metis:std#oid42" label="E-mail" desc="E-mail address of the organization"/>
  <property name="city" xlink:role="string-type" xlink:title="Name with Rich Text" xlink:href="metis:std#oid42" label="City"/>
  <property name="address2" xlink:role="string-type" xlink:title="Name with Rich Text" xlink:href="metis:std#oid42" label="Address 2 (Po. Box)"/>
  <property name="url" xlink:role="string-type" xlink:title="Name with Rich Text" xlink:href="metis:std#oid42" label="URL"/>
