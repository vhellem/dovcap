  <property name="homepage" xlink:role="string-type" xlink:title="string" xlink:href="metis:std#oid3" label="Homepage" desc="Homepage URL"/>
  <property name="type" xlink:role="string-type" xlink:title="Int Ext Classification" xlink:href="../primitive_types/primitive_types.kmd#oid50" label="Int/Ext Classification" desc="Internal/External classification. Typical external organizations are vendors and customers"/>
  <property name="image" xlink:role="string-type" xlink:title="string" xlink:href="metis:std#oid3" label="Image"/>
  <part-rule min="0" max="..." xlink:role="type" xlink:title="Organization" xlink:href="#oid1"/>
  <part-rule min="0" max="..." xlink:role="type" xlink:title="Position" xlink:href="position.kmd#oid1"/>
