 <type id="oid1" name="AllocatedOn" title="allocated on/allocated to" usage="relationship" desc="Allows you to show the allocation of a Datastore, Physical  or Database to a Disk.">
  <complex-type-link xlink:role="type" xlink:href="http://xml.computas.com/xml/user/relshipsharedprop.kmd#oid1"/>
  <connect-rule>
   <origin-rule min="0" max="..." xlink:role="type" xlink:href="../object_types/physical_datastore.kmd#oid1" xlink:actuate="user"/>
   <target-rule min="0" max="..." xlink:role="type" xlink:href="../object_types/disk.kmd#oid1" xlink:actuate="user"/>
  </connect-rule>
  <valueset vset="shared" size="2" xlink:role="type" xlink:href="#oid1">
   <string name="from">allocated on</string>
   <string name="to">allocated to</string>
  </valueset>
 </type>


